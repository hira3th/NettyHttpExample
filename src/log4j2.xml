<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">

	 <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%-5p | %d{yyyy-MM-dd HH:mm:ss} | [%t] (%F:%L): \n\t%m%n"/>
        </Console>
		
	</Appenders>
    
	 <Loggers>

    <Root level="info">
		<AppenderRef ref="STDOUT" />
    </Root></Loggers>

</Configuration>